<%- model_class = Swimset -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:practice_id) %>:</strong></dt>
  <dd><%= @swimset.practice_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:qty) %>:</strong></dt>
  <dd><%= @swimset.qty %></dd>
  <dt><strong><%= model_class.human_attribute_name(:distance) %>:</strong></dt>
  <dd><%= @swimset.distance %></dd>
  <dt><strong><%= model_class.human_attribute_name(:rori) %>:</strong></dt>
  <dd><%= @swimset.rori %></dd>
  <dt><strong><%= model_class.human_attribute_name(:rest) %>:</strong></dt>
  <dd><%= @swimset.rest %></dd>
  <dt><strong><%= model_class.human_attribute_name(:comment) %>:</strong></dt>
  <dd><%= @swimset.comment %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              swimsets_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_swimset_path(@swimset), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              swimset_path(@swimset),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
